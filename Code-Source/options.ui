              <property name="styleSheet">
               <string notr="true">/* Style pour le bouton Filiers - Style moderne d'entreprise */
#filiers {
    background: #ffffff; /* Fond blanc */
    color: #333333; /* Texte gris foncé */
    padding: 15px 25px; /* Espacement autour du texte */
    border: 2px solid #555555; /* Contour gris foncé */
    border-radius: 26px; /* Coins arrondis */
    font: 16px &quot;Helvetica Neue&quot;, sans-serif; /* Police sans-serif moderne */
    cursor: pointer; /* Curseur pointer au survol */
    transition: background 0.3s ease, color 0.3s ease, border-color 0.3s ease; /* Animation de transition sur la couleur de fond, la couleur du texte et la couleur du contour */
}

/* Effet de survol pour le bouton Filiers */
#filiers:hover {
    background: #f1f1f1; /* Fond légèrement plus foncé au survol */
    color: #555555; /* Texte gris un peu plus clair au survol */
    border-color: #777777; /* Contour gris plus clair au survol */
}

/* Effet de pression pour le bouton Filiers */
#filiers:active {
    background: #e6e6e6; /* Fond légèrement plus foncé lorsqu'appuyé */
    box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1); /* Légère ombre lorsqu'appuyé */
    transform: translateY(2px); /* Légère translation vers le bas pour l'effet de pression */
}
</string>
